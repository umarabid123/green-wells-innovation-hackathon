# TrafficWise AI Backend Environment Variables
# Copy this file to .env and fill in your actual API keys

# Google Gemini API (Optional - users can provide via frontend)
GOOGLE_GEMINI_API_KEY=

# OpenAI API (Optional - users can provide via frontend)  
OPENAI_API_KEY=

# FastAPI Configuration
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
FASTAPI_RELOAD=true

# CORS Origins (add your frontend URLs)
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# Logging Level
LOG_LEVEL=INFO

# Database (if implementing persistent storage)
# DATABASE_URL=sqlite:///./trafficwise.db